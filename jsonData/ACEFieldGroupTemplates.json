            "templateOptions": {
              "label": "Country Code",
              "maxLength": 5,
              "validator": "integer",
              "required": true
            },
            "validation": {
              "messages": {
                "required": "\"Country Code required\"",
                "pattern": "\"Invalid Country Code\""
              }
            }
          },
